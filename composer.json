{
    "name": "tomrf/configcontainer",
    "type": "library",
    "description": "configcontainer",
    "keywords": [ "configcontainer" ],
    "homepage": "http://github.com/tomrf/configcontainer",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "roave/security-advisories": "dev-latest",
        "phpmd/phpmd": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Tomrf\\ConfigContainer\\": "src/"
        }
    },
    "scripts": {
        "phpunit": [
            "phpunit --do-not-cache-result --stderr"
        ],
        "cs-fixer": [
            "php-cs-fixer fix --allow-risky=yes --diff --verbose src/"
        ],
        "phpstan": [
            "phpstan analyze --level max src/"
        ],
        "phpmd": [
            "phpmd src/ ansi .phpmd-ruleset.xml"
        ],
        "test": [
            "@phpunit",
            "@phpmd",
            "@phpstan"
        ]
    }
}
